defun rotate_left_x_arr( Array arr, int start, int end, int rot ):
  var temp = arr[start];
  loop i from start to end:
    var ind = i + rot;
    if ind > end:
      ind = ind - ( end - start );
    arr[i] = arr[ind];
  end
  arr[end] = temp;
end

defun do_mult( int a ):
  carry = select( a, 7, 8 );
  a = trunc( a << 1, 8 );
  if carry equals 1:
    a = a ^ 0x1b;
  end
end

defun gf_mult( int a, int b ):
  p = 0;
  loop i from 0 to 7:
    if (b & 1) equals 0:
      p = p ^ a;
    end
    b = b >> 1;
    do_mult( a );
  end
  do_mult(a);
  return p;
end    

mix_cols =
[ [2, 3, 1, 1],
  [1, 2, 3, 1],
  [1, 1, 2, 3],
  [3, 1, 1, 2] ];

defun mix_cols( Array a ):
  out = [0,0,0,0];
  loop row from 0 to 4:
    row_vec = mix_cols[row];
    loop col from 0 to 4:
      out[row] = out[row] + gf_mult( row_vec[col], a[col] );
    end
  end
  return out;
end
